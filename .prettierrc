{
    "printWidth": 120,
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "bracketSpacing": true,
    "bracketSameLine": false,
    "arrowParens": "avoid",
    "importOrder": [
        "^queries",
        "^assets",
        "^@shared",
        "^components",
        "^hooks",
        "^redux",
        "^types",
        "^utils",
        "^[./](.*)(?<!.scss)$",
        "(.*).scss"
    ],
    "importOrderSeparation": false,
    "importOrderSortSpecifiers": true
}
